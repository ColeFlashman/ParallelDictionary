        <Button x:Name="searchBtn" Content="SEARCH" FontSize="30" FontWeight="SemiBold" VerticalAlignment="Center" Height="70" Width="200" MaxHeight="150" MaxWidth="350" Background="Gold" Foreground="White" Click="searchBtn_Click"/>
        <!--<Button x:Name="searchBtn" Content="SEARCH" FontSize="30" FontWeight="SemiBold" VerticalAlignment="Top" Height="70" Width="200" MaxHeight="150" MaxWidth="350" Background="Gold" Foreground="White"/>-->

    </Grid>
</Page>
